#!/usr/bin/make -f

.PHONY: build-arch build-indep build clean binary-arch binary-indep binary clean

build-arch:

build-indep:

build: build-arch build-indep

binary-arch: build

binary-indep: build

binary: binary-arch binary-indep
	dh_installdocs
	dh_installchangelogs
	dh_compress
	dh_gencontrol
	dh_builddeb

clean:
	dh_clean
